generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model url_shortener{
   id  Int @id@default(autoincrement())
   original_url String
   short_code String @unique
   created_at DateTime @default(now())
   visit_count Int @default(0)
   last_accessed_at DateTime?
}

